<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="http://jmri.org/xml/XSLT/aspecttable.xsl" type="text/xsl"?>

<nodes
    xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:noNamespaceSchemaLocation = "http://jmri.org/xml/schema/aspecttable.xsd"
>

  <node><name>File</name>
    <mnemonic>f</mnemonic>
    <node><name>New DecoderPro Window</name>
        <mnemonic>n</mnemonic>
    </node>
    <node><name>Import Roster</name>
    </node>
    <node><name>Import Roster Entry</name>
        <adapter>jmri.jmrit.roster.ImportRosterItemAction</adapter>
    </node>
    <node><name>Import Decoder File</name>
    </node>
    <node><name>Import Decoder URL</name>
    </node>
    <node>separator</node>
    <node><name>Export Roster Entry</name>
        <adapter>jmri.jmrit.roster.ExportRosterItemAction</adapter>
    </node>
    <node><name>Export Complete Roster</name>
        <adapter>jmri.jmrit.roster.rosterExportAction</adapter>
    </node>
    <node>separator</node>
    <node><name>Print Roster Summary</name>
    </node>
    <node><name>Print Roster Entry</name>
        <adapter>jmri.jmrit.roster.PrintRosterAction</adapter>
    </node>
    <node><name>Print Preview Roster Summary</name>
    </node>
    <node><name>Print Preview Roster Entry</name>
        <adapter>jmri.jmrit.roster.PrintRosterAction</adapter>
    </node>
    <node>separator</node>
    <node><name>Quit</name>
        <mnemonic>q</mnemonic>
        <adapter>apps.gui3.paned.QuitAction</adapter>
    </node>
  </node>

  <node><name>Edit</name>
    <mnemonic>e</mnemonic>
    <node><name>Cut</name>
    </node>
    <node><name>Copy</name>
    </node>
    <node><name>Paste</name>
    </node>
    <node><name>Duplicate Loco</name>
    </node>
    <node>separator</node>
    <node><name>Preferences</name>
        <adapter>apps.gui3.TabbedPreferencesAction</adapter>
        <mnemonic>p</mnemonic>
    </node>
  </node>

  <node><name>Settings</name>
    <mnemonic>s</mnemonic>
    <node><name>Summary Pane</name>
    </node>
    <node><name>Reset Window Sizes</name>
    </node>
    <node><name>Hide/Show Roster Image</name>
    </node>
    <node>separator</node>
    <node><name>Programming</name>
        <mnemonic>p</mnemonic>
        <node><name>Service Mode</name></node>
        <node><name>Operations Mode</name></node>
        <node><name>Edit Mode</name></node>
    </node>
    <node>separator</node>
    <node><name>Create Roster Group</name>
    </node>
    <node><name>Delete Roster Group</name>
    </node>
    <node><name>Set Active Roster Group</name>
    </node>
    <node><name>Roster Group Table Association</name>
    </node>
  </node>

  
  
  <node><name>Actions</name>
    <mnemonic>a</mnemonic>
    <node><name>Basic Programmer</name>
    </node>
    <node><name>Comprehencise Programmer</name>
    </node>
    <node><name>Single CV Programmer</name>
    </node>
    <node><name>Edit Throttle Labels</name>
    </node>
    <node><name>Edit Roster Media</name>
    </node>
    <node>separator</node>
    <node><name>New Throttle</name>
        <adapter>jmri.jmrit.throttle.ThrottleCreationAction</adapter>
    </node>
    <node><name>Consisting Tool</name>
        <adapter>jmri.jmrit.consisttool.ConsistToolAction</adapter>
    </node>
    <node>separator</node>
    <node><name>Start WiThrottle Server</name>
        <adapter>jmri.jmrit.withrottle.WiThrottleCreationAction</adapter>
    </node>
    <node><name>Start Mini WebServer</name>
        <adapter>jmri.web.miniserver.MiniServerAction</adapter>
    </node>
    <node>separator</node>
    <node><name>Recreate Roster Index</name>
        <adapter>jmri.jmrit.roster.RecreateRosterAction</adapter>
    </node>
    <node><name>Recreate Decoder Index</name>
        <adapter>jmri.jmrit.decoderdefn.DecoderIndexCreateAction</adapter>
    </node>
    <node>separator</node>
    <node><name>Run Script</name>
        <adapter>jmri.jmrit.jython.RunJythonScript</adapter>
    </node>
  </node>

<!---  <xi:include href="http://jmri.org/xml/config/parts/WindowsMenu.xml"/>
  <xi:include href="http://jmri.org/xml/config/parts/HelpMenu.xml"/>-->

</nodes>
  