<decoder-config>
		<version author="Simon Ginsburg" version="1" lastUpdated="20060722"/>

<decoder>
	<family name="Multiprotocoll Decoder" mfg="Uhlenbrock Elektronik"
	             lowVersionID="9" highVersionID="20">
		<model model="76400" />
		<model model="76420" />
	</family>

	<programming direct="yes" 
				 paged="yes" 
				 register="yes" 
				 ops="yes" />

	<variables>
		<variable label="Primary Address" CV="1" comment="Short address"
						item="Short Address"  default="03">
			<shortAddressVal/></variable>
		<variable label="Vstart" CV="2"
						comment="Speed setting between 1 and 63"
                        default="01">
			<decVal max="63"/></variable>
		<variable label="Acceleration Rate" CV="3"
						comment="A value of 1 will increase each 5 milliseconds by 1"
						item="Accel" 
                        default="02">
			<decVal max="63"/></variable>
		<variable label="Deceleration Rate" CV="4"
						comment="A value of 1 will increase each 5 milliseconds by 1"
						item="Decel"
                        default="02">
			<decVal max="63"/></variable>
		<variable label="Vhigh" CV="5"
						comment="Speed setting between 1 and 63"
                        default="48">
            <decVal max="63"/></variable>
		<variable label="Vmid" CV="6"
						comment="Speed setting between 1 and 63"
                        default="24">
            <decVal max="63"/></variable>
		<variable label="Manufacturer Version No: " CV="7" readOnly="yes"
						item="Decoder Version" default="9"><decVal/></variable>
		<variable label="Manufacturer ID: " CV="8" readOnly="yes"
						item="Manufacturer" default="85">	<decVal/></variable>

			
		<variable label="Extended Address" CV="17"
						item="Long Address" >
			<longAddressVal/></variable>
			
		<variable label="Consist Address" CV="19" mask="XVVVVVVV" item="Consist Address">
			<decVal/></variable>
		
		<variable label="Consist Direction" CV="19" mask="VXXXXXXX">
			<enumVal>
				<enumChoice choice="Normal"></enumChoice>
				<enumChoice choice="Reversed"></enumChoice>
				</enumVal></variable>
		
		<variable label="Locomotive Direction" CV="29" mask="XXXXXXXV">
			<enumVal>
				<enumChoice choice="normal"/>
				<enumChoice choice="reversed"/>
				</enumVal></variable>
		<variable label="FL Location" CV="29" mask="XXXXXXVX"
					item="Speed Step Mode" default="1">
			<enumVal>
				<enumChoice choice="14 speed step format"/>
				<enumChoice choice="28 speed step format"/>
				</enumVal></variable>
		<variable label="Power Source Conversion" CV="29" mask="XXXXXVXX"
					item="Analog (DC) Operation" default="1">
			<enumVal>
				<enumChoice choice="NMRA Digital only"/>
				<enumChoice choice="DC conversion enabled"/>
				</enumVal></variable>
		<variable label="Speed Table Definition" CV="29" mask="XXXVXXXX">
			<enumVal>
				<enumChoice choice="Use Vstart, Vmid, Vhigh"/>
				<enumChoice choice="Use table in CVs 66 through 95"/>
				</enumVal></variable>
		<variable label="Address Format" CV="29" mask="XXVXXXXX">
			<enumVal>
				<enumChoice choice="One byte (short) address"/>
				<enumChoice choice="Two byte (extended) address"/>
				</enumVal></variable>

		<variable label="FL(f) controls front light output" CV="33" mask="XXXXXXXV" minOut="1" default="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(f) controls back light output" CV="33" mask="XXXXXXVX" minOut="2"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(f) controls Special function output 1" CV="33" mask="XXXXXVXX" minOut="3"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(f) controls Special function output 2" CV="33" mask="XXXXVXXX" minOut="4"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(f) controls switching speed" CV="33" mask="XXXVXXXX" minOut="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(f) controls momentum" CV="33" mask="XXVXXXXX" minOut="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>

		<variable label="FL(r) controls front light output" CV="34" mask="XXXXXXXV" minOut="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(r) controls back light output" CV="34" mask="XXXXXXVX" minOut="2" default="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(r) controls Special function output 1" CV="34" mask="XXXXXVXX" minOut="3"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(r) controls Special function output 2" CV="34" mask="XXXXVXXX" minOut="4"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(r) controls switching speed" CV="34" mask="XXXVXXXX" minOut="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="FL(r) controls momentum" CV="34" mask="XXVXXXXX" minOut="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>

		<variable label="F1 controls front light output" CV="35" mask="XXXXXXXV" minOut="1" minFn="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F1 controls back light output" CV="35" mask="XXXXXXVX" minOut="2" minFn="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F1 controls Special function output 1" CV="35" mask="XXXXXVXX" minOut="3" minFn="1" default="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F1 controls Special function output 2" CV="35" mask="XXXXVXXX" minOut="4" minFn="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F1 controls switching speed" CV="35" mask="XXXVXXXX" minOut="5" minFn="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F1 controls momentum" CV="35" mask="XXVXXXXX" minOut="6" minFn="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>

		<variable label="F2 controls front light output" CV="36" mask="XXXXXXXV" minOut="1" minFn="2"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F2 controls back light output" CV="36" mask="XXXXXXVX" minOut="2" minFn="2"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F2 controls Special function output 1" CV="36" mask="XXXXXVXX" minOut="3" minFn="2"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F2 controls Special function output 2" CV="36" mask="XXXXVXXX" minOut="4" minFn="2" default="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F2 controls switching speed" CV="36" mask="XXXVXXXX" minOut="5" minFn="2"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F2 controls momentum" CV="36" mask="XXVXXXXX" minOut="6" minFn="2"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>

		<variable label="F3 controls front light output" CV="37" mask="XXXXXXXV" minOut="1" minFn="3"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F3 controls back light output" CV="37" mask="XXXXXXVX" minOut="2" minFn="3"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F3 controls Special function output 1" CV="37" mask="XXXXXVXX" minOut="3" minFn="3"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F3 controls Special function output 2" CV="37" mask="XXXXVXXX" minOut="4" minFn="3"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F3 controls switching speed" CV="37" mask="XXXVXXXX" minOut="5" minFn="3" default="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F3 controls momentum" CV="37" mask="XXVXXXXX" minOut="6" minFn="3"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>

		<variable label="F4 controls front light output" CV="38" mask="XXXXXXXV" minOut="4" minFn="4"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F4 controls back light output" CV="38" mask="XXXXXXVX" minOut="5" minFn="4"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F4 controls Special function output 1" CV="38" mask="XXXXXVXX" minOut="6" minFn="4"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F4 controls Special function output 2" CV="38" mask="XXXXVXXX" minOut="7" minFn="4"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F4 controls switching speed" CV="38" mask="XXXVXXXX" minOut="8" minFn="4"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F4 controls momentum" CV="38" mask="XXVXXXXX" minOut="9" minFn="4" default="1"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
			
		<variable label="F5 controls front light output" CV="39" mask="XXXXXXXV" minOut="4" minFn="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F5 controls back light output" CV="39" mask="XXXXXXVX" minOut="5" minFn="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F5 controls Special function output 1" CV="39" mask="XXXXXVXX" minOut="6" minFn="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F5 controls Special function output 2" CV="39" mask="XXXXVXXX" minOut="7" minFn="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F5 controls switching speed" CV="39" mask="XXXVXXXX" minOut="8" minFn="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F5 controls momentum" CV="39" mask="XXVXXXXX" minOut="9" minFn="5"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
			
		<variable label="F6 controls front light output" CV="40" mask="XXXXXXXV" minOut="4" minFn="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F6 controls back light output" CV="40" mask="XXXXXXVX" minOut="5" minFn="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F6 controls Special function output 1" CV="40" mask="XXXXXVXX" minOut="6" minFn="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F6 controls Special function output 2" CV="40" mask="XXXXVXXX" minOut="7" minFn="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F6 controls switching speed" CV="40" mask="XXXVXXXX" minOut="8" minFn="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
		<variable label="F6 controls momentum" CV="40" mask="XXVXXXXX" minOut="9" minFn="6"> <enumVal> 
			<enumChoice choice="No"/> <enumChoice choice="Yes"/> </enumVal> </variable>
			
		<variable label="Main Motor Control" CV="49" mask="XXXXXXXV">
			<enumVal>
				<enumChoice choice="on"/>
				<enumChoice choice="off"/>
				</enumVal></variable>
		<variable label="SUSI Configuration" CV="49" mask="XXXXXXVX">
			<enumVal>
				<enumChoice choice="Soundmodule"/>
				<enumChoice choice="LISSY Mini-Sendingmodule"/>
				</enumVal></variable>
		<variable label="Braking End" CV="49" mask="XXXXXVXX">
			<enumVal>
				<enumChoice choice="to standstill"/>
				<enumChoice choice="as def. in CV52"/>
				</enumVal></variable>
		<variable label="Data Format DDC" CV="49" mask="XXXXVXXX">
			<enumVal>
				<enumChoice choice="Motorola and DDC"/>
				<enumChoice choice="DDC Only"/>
				</enumVal></variable>
		<variable label="Data Format Motorola" CV="49" mask="XXXVXXXX">
			<enumVal>
				<enumChoice choice="Motorola and DDC"/>
				<enumChoice choice="Motorola Only"/>
				</enumVal></variable>
		<variable label="Dimming for A1 and A2" CV="49" mask="XXVXXXXX" default="1">
			<enumVal>
				<enumChoice choice="off"/>
				<enumChoice choice="on"/>
				</enumVal></variable>
		<variable label="Lightplugs" CV="49" mask="XVXXXXXX">
			<enumVal>
				<enumChoice choice="straight"/>
				<enumChoice choice="reverse"/>
			</enumVal>
		</variable>
		<variable label="Braking with" CV="49" mask="VXXXXXXX">
			<enumVal>
				<enumChoice choice="Braking signal only"/>
				<enumChoice choice="with analog voltage"/>
			</enumVal>
		</variable>

		<variable label="Function output dimming" CV="50" default="32" >
			<decVal min="0" max="63"/>
		</variable>

		<variable label="Analog/Digitalswitch" CV="51" default="3" >
			<enumVal>
				<enumChoice choice="Not used"/>
				<enumChoice choice="Digital operation"/>
				<enumChoice choice="Analog operation"/>
				<enumChoice choice="Auto Detection"/>
			</enumVal>
		</variable>

		<variable label="End braking speed" CV="52" default="30" 
		           tooltip="Valid when CV49 Bit 2=1 and Bit 7=1">
			<decVal/>
		</variable>
			
		<variable label="Repetition rate" CV="53" default="120" 
		           tooltip="Value * 53 um; useful values 60-255">
			<decVal/>
		</variable>
			
		<variable label="Control Setpoint" CV="56" default="64"
		           tooltip="Lower Value if top speed to low, Raise Value if Speed at Step 1 too fast">
			<decVal/>
		</variable>
			
		<variable label="AD Timeslot" CV="58" default="10" 
		           tooltip="Timeslot for AD converter sensor">
			<decVal/>
		</variable>
			
		<variable label="Short circuit supervision" CV="60" default="250"
					readOnly="yes"
		           tooltip="Do not change! 0=deactivated, 250=activated">
			<decVal/>
		</variable>
			
		<variable label="Temperature switchoff limit" CV="61" default="0" 
		           tooltip="Different with each decoder, 0=deactivated">
			<decVal/>
		</variable>
			
		<variable label="Speed Correction Forward" CV="64" default="128" 
		           tooltip="Factor = Value of CV64/128">
			<decVal/>
		</variable>
			
		<variable label="Speed Correction Reverse" CV="95" default="128" 
		           tooltip="Factor = Value of CV64/128">
			<decVal/>
		</variable>
			
		<variable label="Offset Register" CV="65" default="0" 
					readOnly="yes"
		           tooltip="Used with Motorola programming">
			<decVal/>
		</variable>
			
		<variable label="Page Register" CV="66" default="0" 
					readOnly="yes"
		           tooltip="Used with DDC paged mode programming">
			<decVal/>
		</variable>
			
		<variable label="LISSY Train Category" CV="115" default="1" 
		           tooltip="Value 1 - 4">
			<decVal max="4"/>
		</variable>
			
		<variable label="Speed Table" CV="67">
			<speedTableVal/></variable>

	
	</variables>

  <resets>
  	<factReset label="Reset All CVs" CV="59" default="1"/>
  </resets>

</decoder>

