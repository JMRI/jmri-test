          <enumChoice choice="Yes" value="0">
            <choice>Yes</choice>
          </enumChoice>
          <enumChoice choice="No" value="6">
            <choice>No</choice>
          </enumChoice>
        </enumVal>
        <label>Sound Time Out</label>
