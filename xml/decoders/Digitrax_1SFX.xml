      <variable label="Manufacturer ID" CV="8" readOnly="yes" item="Manufacturer" default="129">
        <decVal/>
        <label>Manufacturer ID</label>
      </variable>
      <xi:include href="http://jmri.org/xml/decoders/digitrax/motorDriveFreq.xml"/>
