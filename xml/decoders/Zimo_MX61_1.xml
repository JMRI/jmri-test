<decoder>
	<family name="Zimo Silent BEMF decoders, early model" mfg="Zimo" >
		<model model="MX61_2000 version 4/10" lowVersionID="4" highVersionID="10" 
 		 maxInputVolts="12-24V" maxMotorCurrent="1A (peak=2A, 5 sec.)"
		 maxTotalCurrent="1.2A" formFactor="HO"
		 numOuts="6" numFns="6" >
	    	<output name="1" label="White" />
	    	<output name="2" label="Yellow" />
	    	<output name="3" label="Brown" />
	    	<output name="4" label="Brown or Green" />
	    	<output name="5" label="NC - Logic Level" />
	    	<output name="6" label="NC - Logic Level" />
	  	</model>
	</family>

	<programming direct="yes" paged="yes"
