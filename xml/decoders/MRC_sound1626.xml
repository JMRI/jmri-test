<decoder-config xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder.xsd">
  <version author="Rick Beaber rbeaber@gmail.com" version="1.1" lastUpdated="20050208"/>
  <decoder>
    <family name="Sound Decoders" mfg="MRC" lowVersionID="1" highVersionID="1">
      <model model="Synch Diesel Sound 1626" numOuts="4" numFns="12"/>
    </family>
    <programming direct="yes" paged="yes" register="yes" ops="yes"/>
    <variables>
      <variable CV="2" item="Vstart" default="0" comment="Range 0-32">
        <decVal max="32"/>
        <label>Start Volts</label>
        <label xml:lang="it">Volt Partenza</label>
        <label xml:lang="fr">V démarr.</label>
        <label xml:lang="de">Startspannung</label>
        <comment>Range 0-32</comment>
      </variable>
      <variable CV="3" item="Accel" default="0" comment="Range 0-32">
        <decVal max="32"/>
        <label>Acceleration momentum</label>
        <label xml:lang="it">Accellerazione (0-32)</label>
        <label xml:lang="fr">Accelération (0-32)</label>
        <label xml:lang="de">Anfahrverzögerung (0-32)</label>
        <comment>Range 0-32</comment>
      </variable>
      <variable CV="4" item="Decel" default="0" comment="Range 0-32">
        <decVal max="32"/>
        <label>Brake momentum</label>
        <label xml:lang="it">Decellerazione (1-32)</label>
        <label xml:lang="fr">Décélération (1-32)</label>
        <label xml:lang="de">Bremszeit (1-32)</label>
        <comment>Range 0-32</comment>
      </variable>
      <variable CV="5" item="Vhigh" default="0" comment="Range 0-32">
        <decVal max="32"/>
        <label>Max Voltage</label>
        <label xml:lang="it">Volt Massimi (0-32):</label>
        <label xml:lang="de">Höchstgeschwindigkeit</label>
        <comment>Range 0-32</comment>
      </variable>
      <variable CV="7" readOnly="yes" item="Decoder Version" default="1">
        <decVal/>
        <label>Version ID</label>
      </variable>
      <variable CV="8" readOnly="yes" item="Manufacturer" default="143">
        <decVal/>
        <label>Manufacturer ID</label>
      </variable>
      <xi:include href="http://jmri.org/xml/decoders/nmra/shortAndLongAddress.xml"/>
      <variable CV="19" mask="XVVVVVVV" item="Consist Address">
        <decVal/>
        <label>Consist Address</label>
      </variable>
      <variable item="Consist Direction" CV="19" mask="VXXXXXXX">
        <enumVal>
          <enumChoice choice="Normal">
            <choice>Normal</choice>
          </enumChoice>
          <enumChoice choice="Reversed">
            <choice>Reversed</choice>
          </enumChoice>
        </enumVal>
        <label>Consist Direction</label>
      </variable>
      <variable CV="29" mask="XXXXXXXV" item="Locomotive Direction">
        <enumVal>
          <enumChoice choice="Forward">
            <choice>Forward</choice>
          </enumChoice>
          <enumChoice choice="Reverse">
            <choice>Reverse</choice>
          </enumChoice>
        </enumVal>
        <label>Normal direction of motion</label>
      </variable>
      <variable CV="29" mask="XXXXXXVX" item="Speed Step Mode" default="1">
        <enumVal>
          <enumChoice choice="14">
            <choice>14</choice>
          </enumChoice>
          <enumChoice choice="28/128 (recommended)">
            <choice>28/128 (recommended)</choice>
          </enumChoice>
        </enumVal>
        <label>Speed Steps</label>
      </variable>
      <variable CV="29" mask="XXXXXVXX" default="1" item="Analog (DC) Operation">
        <enumVal>
          <enumChoice choice="Off">
            <choice>Off</choice>
          </enumChoice>
          <enumChoice choice="On">
            <choice>On</choice>
          </enumChoice>
        </enumVal>
        <label>Analog conversion mode</label>
      </variable>
      <variable CV="29" mask="XXXVXXXX" item="Speed Table Definition">
        <enumVal>
          <enumChoice choice="Don't use Speed Table">
            <choice>Don't use Speed Table</choice>
          </enumChoice>
          <enumChoice choice="Use Speed Table">
            <choice>Use Speed Table</choice>
          </enumChoice>
        </enumVal>
        <label>Use Speed Table</label>
      </variable>
      <variable CV="52" item="Sound Setting 1" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Horn Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="53" item="Sound Setting 2" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Bell Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="54" item="Sound Setting 3" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Diesel Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="55" item="Sound Setting 4" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Brake Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable item="Sound Setting 5" CV="56" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Dynamic Brake Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable item="Sound Setting 6" CV="57" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Brake Release Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="58" item="Sound Setting 7" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Air Pump Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="59" item="Sound Setting 8" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Coupling Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="60" item="Sound Setting A" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Conductor Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="61" item="Sound Setting B" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Reverser Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="62" item="Sound Setting C" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Exhaust Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="63" item="Sound Setting D" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Sand Release Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable CV="64" item="Sound Setting E" default="31" comment="Range 0-31">
        <decVal max="31"/>
        <label>Air Release Volume</label>
        <comment>Range 0-31</comment>
      </variable>
      <variable item="Speed Table" CV="67">
        <speedTableVal/>
        <label>Speed Table</label>
      </variable>
      <variable CV="105" item="User Id #1" default="0">
        <decVal max="255"/>
        <label>User ID #1</label>
      </variable>
      <variable CV="106" item="User Id #2" default="0">
        <decVal max="255"/>
        <label>User ID #2</label>
      </variable>
      <variable CV="50" item="Sound Option 1" default="1" comment="Range 0-2">
        <decVal max="2"/>
        <label>Horn Type</label>
        <comment>Range 0-2</comment>
      </variable>
      <variable CV="51" item="Sound Option 2" default="1" comment="Range 0-2">
        <decVal max="2"/>
        <label>Bell Type</label>
        <comment>Range 0-2</comment>
      </variable>
      <variable item="Global lighting option 3" CV="112" default="0">
        <enumVal>
          <enumChoice choice="Headlights Normal - Pad#1 Ditch - Pad#2 Ditch" value="0">
            <choice>Headlights Normal - Pad#1 Ditch - Pad#2 Ditch</choice>
          </enumChoice>
          <enumChoice choice="Headlights Normal - Pad#1 Mars  - Pad#2 Single Strobe" value="1">
            <choice>Headlights Normal - Pad#1 Mars  - Pad#2 Single Strobe</choice>
          </enumChoice>
          <enumChoice choice="Headlights Normal - Pad#1 Mars  - Pad#2 Dual Strobe" value="2">
            <choice>Headlights Normal - Pad#1 Mars  - Pad#2 Dual Strobe</choice>
          </enumChoice>
          <enumChoice choice="Headlights Rule 17 - Pad#1 Ditch - Pad#2 Ditch" value="16">
            <choice>Headlights Rule 17 - Pad#1 Ditch - Pad#2 Ditch</choice>
          </enumChoice>
          <enumChoice choice="Headlights Rule 17 - Pad#1 Mars  - Pad#2 Single Strobe" value="17">
            <choice>Headlights Rule 17 - Pad#1 Mars  - Pad#2 Single Strobe</choice>
          </enumChoice>
          <enumChoice choice="Headlights Rule 17 - Pad#1 Mars  - Pad#2 Dual Strobe" value="18">
            <choice>Headlights Rule 17 - Pad#1 Mars  - Pad#2 Dual Strobe</choice>
          </enumChoice>
        </enumVal>
        <label>Light Effects</label>
      </variable>
      <variable item="Global lighting option 1" CV="113" default="10" comment="Range 0-48">
        <decVal max="48"/>
        <label>Ditch/Mars Light Rate</label>
        <comment>Range 0-48</comment>
      </variable>
      <variable item="Global lighting option 2" CV="114" default="10" comment="Range 0-48">
        <decVal max="48"/>
        <label>Strobe Light Rate</label>
        <comment>Range 0-48</comment>
      </variable>
      <variable item="Sound Option 3" CV="115">
        <enumVal>
          <enumChoice choice="Off">
            <choice>Off</choice>
          </enumChoice>
          <enumChoice choice="On">
            <choice>On</choice>
          </enumChoice>
        </enumVal>
        <label>Brake squeal</label>
      </variable>
    </variables>
  </decoder>
  <pane>
    <column>
      <row>
        <display item="Sound Setting A" layout="above" label="">
          <label/>
        </display>
        <label>
          <text> </text>
        </label>
        <display item="Sound Setting A" layout="above" format="hslider"/>
      </row>
      <label>
        <text> </text>
      </label>
      <row>
        <display item="Sound Setting B" layout="above" label="">
          <label/>
        </display>
        <label>
          <text> </text>
        </label>
        <display item="Sound Setting B" layout="above" format="hslider"/>
      </row>
      <label>
        <text> </text>
      </label>
      <row>
        <display item="Sound Setting C" layout="above" label="">
          <label/>
        </display>
        <label>
          <text> </text>
        </label>
        <display item="Sound Setting C" layout="above" format="hslider"/>
      </row>
      <label>
        <text> </text>
      </label>
      <row>
        <display item="Sound Setting D" layout="above" label="">
          <label/>
        </display>
        <label>
          <text> </text>
        </label>
        <display item="Sound Setting D" layout="above" format="hslider"/>
      </row>
      <label>
        <text> </text>
      </label>
      <row>
        <display item="Sound Setting E" layout="above" label="">
          <label/>
        </display>
        <label>
          <text> </text>
        </label>
        <display item="Sound Setting E" layout="above" format="hslider"/>
      </row>
    </column>
    <name>Sound Levels-2</name>
  </pane>
</decoder-config>
