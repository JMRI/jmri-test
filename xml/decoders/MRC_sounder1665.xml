      <variable label="Whistle volume" CV="51" item="Sound Setting 1" default="3" tooltip="CV51, range 0-3" comment="Range 0-3">
        <decVal max="3"/>
        <label>Whistle volume</label>
        <comment>Range 0-3</comment>
      </variable>
      <variable label="Bell type" CV="52" item="Sound Option 2" default="3" tooltip="CV52 Note: Bell, Dynamic Brake and Rail Wheel Clack cannot play at the same time" comment="Range 0-6">
