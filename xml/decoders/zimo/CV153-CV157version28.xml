      </enumChoice>
    </enumVal>
    <label>Special Output Configuration</label>
  </variable>
  <variable label="Drive Away if Startup sound still playing" CV="154" mask="XXXXXXVX" default="0">
    <enumVal>
      <enumChoice choice="No">
        <choice>No</choice>
      </enumChoice>
      <enumChoice choice="Yes">
        <choice>Yes</choice>
      </enumChoice>
    </enumVal>
    <label>Drive Away if Startup sound still playing</label>
  </variable>
