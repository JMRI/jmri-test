  <variable label="Max drive Time after signal loss" CV="153" item="Packet Time-out Value" default="0" tooltip="When Energy storage cct connected, 0-255 (0-25sec)">
    <decVal min="0" max="255"/>
    <label>Max drive Time after signal loss</label>
  </variable>
  <variable label="Special Output Configuration" CV="154" mask="XXXXXXXV" default="0">
    <enumVal>
      <enumChoice choice="No Special Configuration">
        <choice>No Special Configuration</choice>
      </enumChoice>
      <enumChoice choice="Pantograph Operation (Zimo/Roco BR110)">
