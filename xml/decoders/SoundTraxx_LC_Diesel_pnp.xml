		<model model="AT100LC/KT100LC Diesel" numOuts="4" numFns="8">
			<versionCV lowVersionID="35"/>
			<output name="Horn"/> 
			<output name="Bell"/> 
			<output name="3" label="FX1"/>
			<output name="4" label="FX2"/>
			<output name="DynBrake"/> 
			<output name="RPM+"/> 
			<output name="RPM-"/> 
			<output name="Dim"/>
			<output name="Mute"/> 
			</model>
		<model model="LL110LC Diesel" numOuts="4" numFns="8">
			<versionCV lowVersionID="35"/>
			<output name="Horn"/> 
			<output name="Bell"/> 
			<output name="3" label="FX1"/>
			<output name="4" label="FX2"/>
			<output name="DynBrake"/> 
			<output name="RPM+"/> 
			<output name="RPM-"/> 
			<output name="Dim"/>
			<output name="Mute"/> 
			</model>
