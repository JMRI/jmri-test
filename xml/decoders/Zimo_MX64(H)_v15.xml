<decoder>
	<family name="Zimo Silent BEMF decoders, late model" mfg="Zimo">
		<model model="MX64(H) version 15+" lowVersionID="15" highVersionID="17" 
		 maxInputVolts="12-24V" maxMotorCurrent="1A(1.5A), peak=2A for 5 sec."
