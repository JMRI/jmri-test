<decoder>
	<family name="DSX Diesel" mfg="Throttle-Up (SoundTraxx)">
		<model model="DSX Diesel" numOuts="0" numFns="8">
			<versionCV lowVersionID="18" highVersionID="19"/>
			<output name="Bell"/> 
			<output name="Horn"/> 
			<output name="RPM+"/> 
			<output name="RPM-"/> 
			<!--output name="3" label="F5/grn"/-->
			<!--output name="4" label="F6/brwn"/-->
