		<variable label="Deceleration" CV="4" default="0"
				comment="Range 0-255">
			<decVal max="255"/>
			</variable>
		
		<variable label="Vhigh" CV="5" default="1"
				comment="Range 0-252">
			<decVal max="252"/>
			</variable>
		
		<variable label="Vmid" CV="6" default="1"
				comment="Range 0-252">
			<decVal max="252"/>
			</variable>

		<variable label="Decoder Version   " CV="7" readOnly="yes"
				item="Decoder Version" >
