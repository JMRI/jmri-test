<decoder-config>
		<version author="sip@euronet.nl"
			version="1.2" lastUpdated="20031008" />
			<!-- Based on various decoder xml files -->

         <!-- V 1.2 changed spelling of "behavior" (Jack Shall) -->

<decoder>
	<family name="Zimo Silent BEMF decoders, late model" mfg="Zimo">
		<model model="MX62 version 4/7" lowVersionID="4" highVersionID="7" 
		 maxInputVolts="12-24V" maxMotorCurrent="0.7A (peak=1.5A, 5 sec.)"
		 maxTotalCurrent="0.8A" formFactor="N, HOe, HOm"
		 numOuts="4" numFns="6" connector="9pin" >
		<size length="0.55" width="0.35" height="0.10" units="inches" />
		<output name="1" label="White" />
	    <output name="2" label="Yellow" />
	    <output name="3" label="NC - Logic Level" />
	    <output name="4" label="NC - Logic Level" />
	  	</model>
	</family>

	<programming direct="yes" paged="yes"
	                     register="yes" ops="yes"></programming>

	<variables>

		<variable label="Primary Address" CV="1" comment="Short address"
				item="Short Address"  default="03">
			<shortAddressVal/></variable>

		<variable label="Vstart" CV="2" default="2"
				comment="Range 0-252">
			<decVal max="252"/>
			</variable>
		
		<variable label="Acceleration" CV="3" default="0"
				comment="Range 0-255">
