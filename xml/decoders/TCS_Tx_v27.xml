<decoder-config>
		<version author="jsalemi@speakeasy.net"
					version="1.2" lastUpdated="20041213"/>

		<!-- Version 1 - made from TCS_Tx, currently for new T1 w/some X functions only.
			New: added CV15 & CV16 (locking); CV62 (momentary pulse control);
			CV22 (light control in MU); CV13 (funtions in analog);
			fixed CV61 (button/dimming controls); and fixed some item descriptions - joe -->
		<!-- Version 1.1 - Added tooltips; corrected CV22 info; enhanced CV61 info. - joe -->
		<!-- Version 1.2 - Completed new F-key re-mapping pane to work with the new non-standard
			 way TCS does it. - joe -->

<decoder>
	<family name="TCS Tx series decoders" mfg="Train Control Systems">
		<model model="T1x" numOuts="2" numFns="2" lowVersionID="27">
			<output name="4" label="Purple"/> </model>
	</family>

	<programming direct="yes" paged="yes"
	                     register="yes" ops="yes"></programming>

	<variables>
		<variable label="Primary Address" CV="1" comment="Short address"
						item="Short Address"  default="03">
			<shortAddressVal/></variable>
		<variable label="Start voltage" CV="2" item="Vstart"
						tooltip="Superseded by Dither.">
			<decVal></decVal>
			</variable>
		<variable label="Acceleration Rate" CV="3" item="Accel">
			<decVal max="255"/>
			</variable>
		<variable label="Deceleration Rate" CV="4" item="Decel">
			<decVal max="255"/>
			</variable>
		<variable label="Maximum Voltage" CV="5" item="Vhigh">
			<decVal/>
			</variable>
		<variable label="Midpoint Voltage" CV="6" item="Vmid">
			<decVal/>
			</variable>
		<variable label="Version ID" CV="7" readOnly="yes" item="Decoder Version"
							default="27">
				<decVal/>
			</variable>
		<variable label="Manufacturer ID" CV="8" readOnly="yes" item="Manufacturer"
