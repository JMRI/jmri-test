			comment="A value of 255 corresponds to 100%">
			<decVal/>
		</variable>
		
		<variable label="Acceleration" CV="3" default="4"
			item="Acceleration" >
			<decVal max="63"/>
		</variable>
		
		<variable label="Deceleration" CV="4" default="3"
			item="Decel">
			<decVal max="63"/>
		</variable>
		
		<variable label="Vhigh" CV="5" default="63"
			comment="Range 0-255">
			<decVal/>
		</variable>
		
		<variable label="Vmid" CV="6" default="25"
