<decoder-config showEmptyPanes="default">
  <version author="Ilidio Serra" version="1" lastUpdated="20090825" />
  <!-- made from the MRC_soundbrilliance1636 file by Bob Jacobsen-->
  <decoder>
    <family name="Brilliance Sound Decoders" mfg="MRC" lowVersionID="32" highVersionID="32" type="TurboTrain">
      <model model="Rapido TurboTrain" numOuts="0" numFns="26" maxTotalCurrent="0.75A" extFnsMRC="yes" nmraWarrant="no" connector="unspecified" extFnsNmraF13="no" extFnsNmraBSC="no">
        <!-- note only F0 to F12 are NMRA compatible -->
        <size length="0" width="0" height="0" units="mm" />
      </model>
    </family>
    <programming direct="yes" paged="yes" register="yes" ops="yes" transpRead="no" bidirRead="no" opsaccbasic="no" opsaccextended="no" opsacclegacy="no" sv="no"></programming>
    <variables>
      <variable label="Primary Address" CV="1" comment="Range 1 - 127" item="Short Address" default="3" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <shortAddressVal />
      </variable>
      <variable label="Start Volts" CV="2" item="Vstart" default="0" comment="Range 0-32" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="32" min="0" />
      </variable>
      <variable label="Acceleration momentum" CV="3" item="Accel" default="0" comment="Range 0-32" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="32" min="0" />
      </variable>
      <variable label="Deceleration momentum" CV="4" item="Decel" default="0" comment="Range 0-32" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="32" min="0" />
      </variable>
      <variable label="Max Voltage" CV="5" item="Vhigh" default="0" comment="Range 0-32" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="32" min="0" />
      </variable>
      <variable label="Speed curve select" CV="6" item="Speedcurve" default="0" comment="Range 0-32" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="Linear Speed Curve"></enumChoice>
          <enumChoice choice="Slow increase at slow speed" />
          <enumChoice choice="Fast increase at slow speed" />
        </enumVal>
      </variable>
      <variable label="Version ID" CV="7" readOnly="yes" item="Decoder Version" default="32" mask="VVVVVVVV" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal min="0" max="255" />
      </variable>
      <variable label="Manufacturer ID" CV="8" readOnly="yes" item="Manufacturer" default="143" mask="VVVVVVVV" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal min="0" max="255" />
      </variable>
      <variable label="Long Address" CV="17" item="Extended Address" comment="4 digit address (Range 0001-9999)" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <longAddressVal />
      </variable>
      <variable label="Advance Consist" CV="19" comment="Range 1 - 127" item="Advanced Consist" default="0" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <AdvanceConsistVal />
      </variable>
      <variable label="Functions Respond To Address" CV="21" mask="XXXXXXXV" default="0" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="all accessory functions will follow its own address" />
          <enumChoice choice="all functions will follow the consist address" />
        </enumVal>
      </variable>
      <variable label="Basic configuration" CV="29" mask="XXXXXXXV" readOnly="no" item="Train Direction" default="2" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal min="0" max="255" />
      </variable>
      <variable label="Speed Steps" CV="29" mask="XXXXXXVX" item="Speed Step Mode" default="1" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="14" />
          <enumChoice choice="28/128 (recommended)" />
        </enumVal>
      </variable>
      <variable label="Analog conversion mode" CV="29" mask="XXXXXVXX" default="1" item="Analog (DC) Operation" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="Off" />
          <enumChoice choice="On" />
        </enumVal>
      </variable>
      <variable label="Use Speed Table" CV="29" mask="XXXVXXXX" item="Speed Table Definition" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="Don't use Speed Table" />
          <enumChoice choice="Use Speed Table" />
        </enumVal>
      </variable>
      <variable label="Addressing Mode" CV="29" mask="XXVXXXXX" item="Address Format" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="2 digit addressing" />
          <enumChoice choice="4 digit addressing" />
        </enumVal>
      </variable>
      <variable label="Sound On/Off" CV="49" mask="XXXXXXXV" default="1" item="Sound Option 1" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="Off" />
          <enumChoice choice="On" />
        </enumVal>
      </variable>
      <variable label="Announcement volume" CV="50" item="Sound Setting 1" default="3" comment="Range 0-3" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="3" min="0" />
      </variable>
      <variable label="Horn Volume" CV="51" item="Sound Setting 2" default="3" comment="Range 0-3" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="3" min="0" />
      </variable>
      <variable label="Bell Volume" CV="53" item="Sound Setting 3" default="3" comment="Range 0-3" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="3" min="0" />
      </variable>
      <variable label="Bell Ring Rate" CV="54" item="Sound Setting 4" default="0" comment="Range 0-1" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="1" min="0" />
      </variable>
      <variable label="Turbine Volume" CV="55" item="Sound Setting 5" default="2" comment="Range 0-4 (4=engine sound off)" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="4" min="0" />
      </variable>
      <variable label="Brake Squeal Volume" CV="56" item="Sound Setting 6" default="3" comment="Range 0-3" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="3" min="0" />
      </variable>
      <variable label="Air release volume" item="Sound Setting 7" CV="57" default="3" comment="Range 0-3" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="3" min="0" />
      </variable>
      <variable label="Door open/close volume" item="Sound Setting 8" CV="58" default="3" comment="Range 0-3" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="3" min="0" />
      </variable>
      <variable label="American/Cananda version" CV="59" item="Sound Setting 8" default="0" comment="American = 0/Cananda = 1" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="American" />
          <enumChoice choice="Canadian" />
        </enumVal>
      </variable>
      <variable label="User ID #1" CV="105" item="User Id #1" default="0" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="255" min="0" />
      </variable>
      <variable label="User ID #2" CV="106" item="User Id #2" default="0" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <decVal max="255" min="0" />
      </variable>
      <variable label="Select Speed Curve" item="Speed Table Selection" CV="124" default="0" mask="VVVVVVVV" readOnly="no" infoOnly="no" opsOnly="no" writeOnly="no">
        <enumVal>
          <enumChoice choice="Linear" />
          <enumChoice choice="Slow increase at slow speed" />
          <enumChoice choice="Fast increase at slow speed" />
        </enumVal>
      </variable>
    </variables>
    <resets>
      <factReset label="Reset All CVs" CV="125" default="0" />
    </resets>
  </decoder>
  <pane name="MRC" nameFmt="label">
    <column>
      <display item="Functions Respond To Address" layout="left" format="default" />
    </column>
  </pane>
</decoder-config>
 	  	 
