			</row>
			<display name="Forward Trim"/>
			<display name="Reverse Trim"/>
		</column>
		<column>
			<display name="Speed Table Definition" 
					format="onradiobutton" layout="above" label=""/>
			<display name="Speed Table" layout="above"/>
		</column>
