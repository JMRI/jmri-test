			<display item="Decoder Version"
				tooltip="The decoder version number (read only)" />
			<display item="Product number"
				tooltip="The decoder product number (if used) (read only)" />
		</column>
	</pane>

	<pane name="Motor">
		<column>
			<display item="Kick Start"
				tooltip="Provides an initial higher voltage spike to loco to overcome starting friction" />
