<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>JMRI</name>
            <properties>
                <property name="ant.script">build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>JMRI</label>
                    <location>.</location>
                </source-folder>
                <source-folder>
                    <label>tmp</label>
                    <type>java</type>
                    <location>java/tmp</location>
                </source-folder>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>java/src</location>
                </source-folder>
                <source-folder>
                    <label>test</label>
                    <type>java</type>
                    <location>java/test</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>debug</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <script>${ant.script}</script>
                    <target>javadoc</target>
                </action>
                <action name="run">
                    <script>${ant.script}</script>
                    <target>decoderpro</target>
                </action>
                <action name="profile">
                    <script>${ant.script}</script>
                    <target>profile-nb</target>
                </action>
                <action name="test">
                    <script>nbproject/ide-targets.xml</script>
                    <target>alltest-nb</target>
                </action>
                <action name="rebuild">
                    <script>${ant.script}</script>
                    <target>clean</target>
                    <target>debug</target>
                </action>
                <action name="compile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>compile-selected-files-in-jmri</target>
                    <context>
                        <property>files</property>
                        <folder>java/src/jmri</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="debug">
                    <script>nbproject/ide-targets.xml</script>
                    <target>debug-nb</target>
                </action>
                <action name="compile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>compile-selected-files-in-src</target>
                    <context>
                        <property>files</property>
                        <folder>java/src</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="compile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>compile-selected-files-in-test</target>
                    <context>
                        <property>files</property>
                        <folder>java/test</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="compile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>compile-selected-files-in-tmp</target>
                    <context>
                        <property>files</property>
                        <folder>java/tmp</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>java/classes</location>
                <script>${ant.script}</script>
                <build-target>debug</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>java/classes</location>
                <script>${ant.script}</script>
                <build-target>debug</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>tmp</label>
                        <location>java/tmp</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>java/src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>test</label>
                        <location>java/test</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="profile"/>
                    <ide-action name="test"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="debug"/>
                    <action>
                        <script>${ant.script}</script>
                        <label>Make Jar File</label>
                        <target>jar</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/3">
            <compilation-unit>
                <package-root>java/src</package-root>
                <classpath mode="compile">java/tmp;lib/ch.ntb.usb.jar;lib/comm-rxtx.jar;lib/xercesImpl.jar;lib/gluegen-rt.jar;lib/javacsv.jar;lib/jdom.jar;lib/jhall.jar;lib/jinput.jar;lib/jmdns.jar;lib/joal.jar;lib/openlcb.jar;lib/junit.jar;lib/jython.jar;lib/log4j.jar;lib/Serialio.jar;lib/servlet.jar;lib/vecmath.jar;lib/activation.jar;lib/mailapi.jar;lib/smtp.jar;lib/jfcunit.jar;lib/jakarta-regexp-1.5.jar;lib/annotations.jar;lib/jsr305.jar;lib/WinRegistry-4.4.jar;lib/jmdns.jar;lib/AppleJavaExtensions.jar;lib/nrjavaserial-3.7.5.jar</classpath>
                <built-to>java/classes</built-to>
                <javadoc-built-to>java/doc</javadoc-built-to>
                <source-level>1.6</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>java/test</package-root>
                <unit-tests/>
                <classpath mode="compile">java/tmp;java/classes;lib/junit.jar;lib/activation.jar;lib/ch.ntb.usb.jar;lib/comm-rxtx.jar;lib/xercesImpl.jar;lib/gluegen-rt.jar;lib/jakarta-regexp-1.5.jar;lib/javacsv.jar;lib/jdom.jar;lib/jfcunit.jar;lib/jhall.jar;lib/jinput.jar;lib/jmdns.jar;lib/joal.jar;lib/openlcb.jar;lib/junit.jar;lib/jython.jar;lib/log4j.jar;lib/mailapi.jar;lib/jython.jar;lib/Serialio.jar;lib/servlet.jar;lib/vecmath.jar;lib/annotations.jar;lib/jsr305.jar;lib/nrjavaserial-3.7.5.jar</classpath>
                <built-to>java/classes</built-to>
                <source-level>1.6</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>java/tmp</package-root>
                <classpath mode="compile">java/tmp;java/classes;lib/junit.jar;lib/activation.jar;lib/ch.ntb.usb.jar;lib/comm-rxtx.jar;lib/xercesImpl.jar;lib/gluegen-rt.jar;lib/jakarta-regexp-1.5.jar;lib/javacsv.jar;lib/jdom.jar;lib/jfcunit.jar;lib/jhall.jar;lib/jinput.jar;lib/jmdns.jar;lib/joal.jar;lib/openlcb.jar;lib/junit.jar;lib/jython.jar;lib/log4j.jar;lib/mailapi.jar;lib/jython.jar;lib/Serialio.jar;lib/servlet.jar;lib/vecmath.jar;lib/annotations.jar;lib/jsr305.jar;lib/nrjavaserial-3.7.5.jar</classpath>
                <source-level>1.6</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
